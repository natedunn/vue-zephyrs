{
  "name": "@natedunn/vue-zephyrs",
  "version": "0.1.15",
  "private": false,
  "description": "A Vue component library for Vue & Tailwind CSS.",
  "repository": "git://github.com/natedunn/vue-zephyrs.git",
  "author": {
    "name": "Nate Dunn",
    "email": "hello@natedunn.net",
    "url": "https://twitter.com/natedunn"
  },
  "scripts": {
    "build": "vue-cli-service build --report --target lib --name vue-zephyrs ./src/index.js",
    "lint": "vue-cli-service lint",
    "docs:build": "vuepress build docs",
    "docs:dev": "vuepress dev docs",
    "release": "sh deploy.sh"
  },
  "main": "dist/vue-zephyrs.umd.min.js",
  "files": [
    "src",
    "dist/*.{js,css}"
  ],
  "release-it": {
    "github": {
      "release": true,
      "releaseName": "${version}"
    },
    "npm": {
      "publish": false
    }
  },
  "dependencies": {
    "@bytegem/vue-heroicons": "^0.5.0",
    "@tailwindcss/ui": "^0.3.0",
    "@trevoreyre/autocomplete-vue": "^2.2.0",
    "lodash": "^4.17.15",
    "postcss-preset-env": "^6.7.0",
    "strip-indent": "^3.0.0",
    "tailwindcss": "^1.4.6",
    "vue": "^2.6.11",
    "vue-fragment": "^1.5.1",
    "vue-multipane": "^0.9.5",
    "vue-prism-component": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.0",
    "@vue/babel-preset-app": "^4.3.1",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "moment": "^2.26.0",
    "node-sass": "^4.14.1",
    "release-it": "^13.6.3",
    "sass-loader": "^8.0.2",
    "tailwindcss-dark-mode": "^1.1.4",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.5.2"
  },
  "peerDependencies": {
    "moment": "^2.24.0"
  }
}
