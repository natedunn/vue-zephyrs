{
  "name": "@natedunn/vue-zephyrs",
  "version": "0.0.8-alpha",
  "private": false,
  "description": "A Vue component library for Vue & Tailwind CSS.",
  "repository": {
    "type": "git",
    "url": "https://github.com/natedunn/vue-zephyrs.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "author": {
    "name": "Nate Dunn",
    "email": "hello@natedunn.net",
    "url": "https://twitter.com/natedunn"
  },
  "scripts": {
    "build": "vue-cli-service build --report --target lib --name vue-zephyrs ./src/index.js",
    "lint": "vue-cli-service lint",
    "docs:build": "vuepress build docs",
    "docs:dev": "vuepress dev docs",
    "release": "source .env && export GITHUB_TOKEN && npx release-it"
  },
  "main": "dist/vue-zephyrs.umd.min.js",
  "files": [
    "src",
    "dist/*.{js,css}"
  ],
  "release-it": {
    "github": {
      "release": true
    }
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "postcss-preset-env": "^6.7.0",
    "strip-indent": "^3.0.0",
    "tailwindcss": "^1.2.0",
    "vue": "^2.6.11",
    "vue-fragment": "^1.5.1",
    "vue-multipane": "^0.9.5",
    "vue-prism-component": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@vue/babel-preset-app": "^4.1.1",
    "@vue/cli-plugin-babel": "^4.1.1",
    "@vue/cli-plugin-eslint": "^4.1.1",
    "@vue/cli-service": "^4.1.1",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.1",
    "moment": "^2.24.0",
    "node-sass": "^4.13.1",
    "release-it": "^12.5.0",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.3.1"
  },
  "peerDependencies": {
    "moment": "^2.24.0"
  }
}
